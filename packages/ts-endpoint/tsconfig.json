{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "composite": true,
    "target": "es5",
    "lib": ["esnext"],
    "outDir": "./lib",
    "baseUrl": "./",
    "rootDir": "./src",
    "tsBuildInfoFile": "./lib/.tsbuildinfo",
    "esModuleInterop": true,
    "paths": {
      "ts-io-error/lib/*": ["../../ts-io-error/src/*"]
    }
  },
  "include": ["./src"],
  "exclude": ["node_modules", "**/*.spec.*", "**/*.typespec.*"],
  "references": [
    {
      "path": "../ts-io-error"
    }
  ]
}
